<launch>
	<node pkg="rosserial_python" name="arduino_driver" type="serial_node.py" args="/dev/ttyUSB0" output="screen"/>
	
	<param name="joy_node/dev" value="/dev/input/js1"/>
	<node pkg="joy" name="xbox_driver" type="joy_node"/>
	
	<node pkg="nmea_navsat_driver" name="gps_driver" type="nmea_topic_serial_reader" args="_port:=/dev/ttyUSB1 _baud:=9600"/>
</launch>
